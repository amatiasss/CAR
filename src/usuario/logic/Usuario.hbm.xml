<?xml version="1.0" encoding="iso-8859-1"?>
<!DOCTYPE hibernate-mapping PUBLIC "-//Hibernate/Hibernate Mapping DTD 3.0//EN" "http://hibernate.sourceforge.net/hibernate-mapping-3.0.dtd">

<hibernate-mapping   package="usuario.logic">
  <class name="Usuario" table="usuario"   optimistic-lock="none">
    <id name="codigoUsuario" type="java.lang.Long" unsaved-value="null" column="codigoUsuario">
      <generator class="native"/>
    </id>
    <property name="nome" type="java.lang.String" column="nome"/>
    <property name="email" type="java.lang.String" column="email"/>
    <property name="ramal" type="java.lang.String" column="ramal"/>       
  </class>
</hibernate-mapping>
